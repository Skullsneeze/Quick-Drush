# Reinstall your drupal site.
# Enter the domain on which your install is located, this will open in your
# default browser when you're ready to install.
# Drops all tables in your database (requires mysql to work).
# Resets your settings file.
dr_reinstall() {
  echo -n 'Reinstall your site? (y/n): '
  read prompt
  if [ "$prompt" == "y" ] || [ "$prompt" == "Y" ]; then
    echo -n 'Website address to return to (without http://): '
    read location
    if [ "$location" == "" ]; then
      echo "No address provided. Using localhost."
      location='localhost'
    fi
    protocol='http://'
    domain=$location
    site=$protocol$domain
    drush sql-drop -y
    echo "Resetting default settings."
    cd sites/default; rm settings.php -f; cp default.settings.php settings.php; cd ../..
    echo "All done, opening your site!"
    start $site
  elif [ "$prompt" == "n" ] || [ "$prompt" == "N" ]; then
    echo 'Aborting!'
    kill -INT $$
  else
    echo 'Invalid answer given. Please answer with "y" or "n"'
    reinstall
    kill -INT $$
  fi
};

# Makes a specific project withing your site.
dr_mkp() {
    if [ -z "$1" ]; then
      echo 'No makefile defined. please use "mkp makefile.make"'
      kill -INT $$
    fi
    echo -n 'What project would you like to make?: '
    read project
    if [ -z "$project" ]; then
      echo 'Invalid input!'
      mkp
      kill -INT $$
    fi
    echo "making $project from $1"
    drush make --projects="$project" --no-core "$1" -y
};

# Make command that prompts to also clear the contrib module folder
dr_mk() {
    if [ -z "$1" ]; then
      echo 'No makefile defined. please use "mk makefile.make"'
      kill -INT $$
    fi
    echo -n 'Would you also like to clear all contrib modules? (y/n): '
    read prompt
    if [ "$prompt" == "y" ] || [ "$prompt" == "Y" ]; then
      echo 'Clearing Contrib modules'
      cd sites/all/modules/contrib; rm -rf *; cd ../../../..
      drush make $1 -y
    elif [ "$prompt" == "n" ] || [ "$prompt" == "N" ]; then
      echo 'Making without clearing the contrib folder.'
      drush make $1 -y
    else
      echo 'Invalid answer given. Please answer with "y" or "n"'
      mk $1
      kill -INT $$
    fi
};

# Same as the above function, but this instantly clears the contrib folder
dr_cmk() {
    if [ -z "$1" ]; then
      echo 'No makefile defined. please use "mk makefile.make"'
      kill -INT $$
    fi
    echo 'Clearing Contrib modules and building site'
    cd sites/all/modules/contrib; rm -rf *; cd ../../../..
    drush make $1 -y
};

